{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "commonjs": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "gsap"
  ],
  "globals": {
    "cy": true
  },
  "rules": {
    "prettier/prettier": 1,
    "no-class-assign": "off",
    "import/no-named-as-default": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off"
  },
  "plugins": ["prettier"]
}
